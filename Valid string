https://www.codewars.com/kata/52f3bb2095d6bfeac2002196/train/javascript

var validWord = function(dictionary, str) {
    const dp = Array(str.length + 1).fill(false);
    dp[0] = true;

    for (let i = 1; i <= str.length; i++) {
        for (let j = 0; j < i; j++) {
            if (dp[j] && dictionary.includes(str.substring(j, i))) {
                dp[i] = true;
                break;
            }
        }
    }

    return dp[str.length];
};
