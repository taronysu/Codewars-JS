var humanYearsCatYearsDogYears = function(humanYears) {
  let catYears=0;
  let dogYears=0;
  let first = 15;
  let second = 9;
  if(humanYears === 1) {
    catYears +=first;
  dogYears +=first;
  }
  else if(humanYears === 2) {
    catYears = first + second;
    dogYears = first + second;
  }
  else if(humanYears>=3) {
    catYears= first + second + 4*humanYears-8;
  dogYears = first + second + 5*humanYears-10;
  }
  
  
  
  return [humanYears,catYears,dogYears];
}
