function euclideanDistance(point1, point2){
let dist =0;
  if (point1.length != point2.length) return -1;
	for (let i = 0; i < point1.length; i++) {
		dist += Math.pow(point1[i] - point2[i], 2);
	}
	return Math.round(Math.sqrt(dist)*100)/100;
}
