https://www.codewars.com/kata/57bf599f102a39bb1e000ae5/train/javascript

var fibsFizzBuzz = function(n) {
let f = [1, 1];
    for (let i = 2; i < n; i++) {
        f.push(f[i - 1] + f[i - 2]);
    }
    for (let i = 0; i < f.length; i++) {
        if (f[i] % 15 === 0) {
            f[i] = 'FizzBuzz';
        } else if (f[i] % 3 === 0) {
            f[i] = 'Fizz';
        } else if (f[i] % 5 === 0) {
            f[i] = 'Buzz';
        }
    }
    return f.slice(0, n);
}
