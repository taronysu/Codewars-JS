function validateBase(num, base) {
const valid = "0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZ".slice(0, base);
    for (let d of String(num)) {
        if (!valid.includes(d)) {
            return false;
        }
    }
  return true;
}
