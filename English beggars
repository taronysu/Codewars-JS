https://www.codewars.com/kata/59590976838112bfea0000fa/train/javascript

function beggars(values, n){
    const result = [];
    for (let i = 0; i < n; i++) {
        let sum = 0;
        for (let j = i; j < values.length; j += n) {
            sum += values[j];
        }
        result.push(sum);
    }
    return result;
}
