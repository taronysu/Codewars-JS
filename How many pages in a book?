https://www.codewars.com/kata/622de76d28bf330057cd6af8/train/javascript

function amountOfPages(summary){
   if(summary <= 9)
     return summary;
  let count = 0;
  let res = 0;
  for(let i = 1; i < summary; i++){
    count += i.toString().length;
    if(count == summary)
      res = i;
  }
  return res;
}
