https://www.codewars.com/kata/59ca8e8e1a68b7de740001f4/train/javascript

function solve(a,b){
  let arr = [];
  let count = 0;
    for (let i = 0; i < b.length; i++) {
count = 0;
      for (let j = 0; j < a.length; j++) {
        if (a[j] === b[i])
          count++;
      }
    arr.push(count);
    }
  return arr;
}
