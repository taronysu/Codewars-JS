https://www.codewars.com/kata/534ffb35edb1241eda0015fe/train/javascript

function scoreHand(cards) {
let total = 0;
    let numberOfAces = 0;
    for (let i of cards) {
        if (i === "A") {
            total += 1;
            numberOfAces += 1;
        } else if (i === "J" || i === "Q" || i === "K") {
            total += 10;
        } else {
            total += parseInt(i);
        }
    }
    if (total <= 11 && numberOfAces >= 1) {
        total += 10;
    }
    return total;
}
