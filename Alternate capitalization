https://www.codewars.com/kata/59cfc000aeb2844d16000075/train/javascript

function capitalize(s){
let first = '';
    let second = '';
    
    for (let i = 0; i < s.length; i++) {
        if (i % 2 === 0) {
            first += s[i].toUpperCase();
            second += s[i];
        } else {
            first += s[i];
            second += s[i].toUpperCase();
        }
    }
    
    return [first, second];
};
