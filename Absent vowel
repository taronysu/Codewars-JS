function absentVowel(x){
  if (!x.match(/a/)) {
    return 0;
    } else if(!x.match(/e/)) {
    return 1;
    } else if(!x.match(/i/)) {
    return 2;
      } else if(!x.match(/o/)) {
    return 3;
        }
    else if(!x.match(/u/)) {
    return 4;
}
      }
